{
  "app": "node app.js",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "**/*.d.ts",
      "**/*.js",
      "tsconfig.json",
      "package*.json",
      "yarn.lock",
      "node_modules",
      "test"
    ]
  },
  "context": {
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/core:checkSecretUsage": true,
    "@aws-cdk/core:target": "aws-cdk-lib@2.0.0",
    "@aws-cdk-lib/aws-lambda:codeSigning": true,
    "@aws-cdk-lib/aws-iam:minimizePolicies": true,
    "@aws-cdk-lib/aws-apigateway:usagePlanKeyOrderInsensitiveId": true,
    "@aws-cdk-lib/aws-rds:lowercaseDbIdentifier": true,
    "@aws-cdk-lib/aws-efs:denyAnonymousAccess": true,
    "@aws-cdk-lib/aws-opensearchservice:enableLogging": true,
    "@aws-cdk-lib/aws-sns:fifoTopicWithoutContentBasedDeduplication": true,
    "@aws-cdk-lib/aws-redshift:columnId": true,
    "@aws-cdk-lib/aws-stepfunctions-tasks:enableLogging": true,
    "@aws-cdk-lib/aws-ec2:uniqueImdsv2TemplateName": true,
    "@aws-cdk-lib/aws-iam:standardizedServicePrincipals": true,
    "@aws-cdk-lib/aws-ecs:arnFormatIncludesClusterName": true,
    "@aws-cdk-lib/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true,
    "@aws-cdk-lib/core:validateSnapshotRemovalPolicy": true,
    "@aws-cdk-lib/aws-codebuild:privilegedModeRequiredForDockerAssets": true,
    "@aws-cdk-lib/aws-events:eventsTargetQueueSameAccount": true,
    "@aws-cdk-lib/aws-iam:principalWithoutCondition": true,
    "@aws-cdk-lib/aws-codepipeline:crossAccountKeyAliasStackSafeResourceName": true,
    "@aws-cdk-lib/aws-s3:createDefaultLoggingPolicy": true,
    "@aws-cdk-lib/aws-sns-subscriptions:restrictSqsDescryption": true,
    "@aws-cdk-lib/aws-apigateway:disableCloudWatchRole": true,
    "@aws-cdk-lib/aws-ecs:removeDefaultDeploymentAlarm": true,
    "@aws-cdk-lib/aws-vpcLattice:addDefaultSecurityGroupIngressRule": true,
    "@aws-cdk-lib/aws-rds:auroraClusterChangeScopeOfInstanceParameterGroupWithEachParameters": true,
    "@aws-cdk-lib/aws-appsync:useArnForSourceApiAssociationIdentifier": true,
    "@aws-cdk-lib/aws-rds:preventRenderingDeprecatedCredentials": true,
    "@aws-cdk-lib/aws-codebuild:avoidImplicitRoleCreation": true,
    "@aws-cdk-lib/aws-opensearch:enableVersionUpgrade": true,
    "@aws-cdk-lib/aws-lambda:useLatestRuntimeVersion": true
  }
}