# AutoSpecAI Environment Configuration
# Copy this file to .env and fill in your actual values

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=your-account-id-here

# API Configuration
API_BASE_URL=https://your-api-gateway-url.execute-api.us-east-1.amazonaws.com/prod
API_VERSION=v1

# API Keys (Generate new keys for production)
AUTOSPEC_API_KEY_1=your-api-key-1-here
AUTOSPEC_API_KEY_2=your-api-key-2-here
AUTOSPEC_API_KEY_3=your-api-key-3-here

# Email Configuration
AUTOSPEC_EMAIL=your-email@example.com
SES_DOMAIN=your-domain.com

# Third-Party Integrations
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL
SLACK_SIGNING_SECRET=your-slack-signing-secret
JIRA_API_TOKEN=your-jira-api-token
GITHUB_TOKEN=ghp_your-github-token
SERVICENOW_USER=your-servicenow-user
SERVICENOW_PASSWORD=your-servicenow-password
SALESFORCE_CLIENT_ID=your-salesforce-client-id
SALESFORCE_CLIENT_SECRET=your-salesforce-client-secret
SALESFORCE_USERNAME=your-salesforce-username
SALESFORCE_PASSWORD=your-salesforce-password
SALESFORCE_SECURITY_TOKEN=your-salesforce-security-token
TEAMS_WEBHOOK_URL=https://your-teams-webhook-url

# LDAP Configuration
LDAP_HOST=ldap.example.com
LDAP_PORT=389
LDAP_BIND_DN=cn=service-account,dc=example,dc=com
LDAP_BIND_PASSWORD=your-ldap-password
LDAP_BASE_DN=dc=example,dc=com

# OAuth Configuration
OAUTH_CLIENT_ID=your-oauth-client-id
OAUTH_CLIENT_SECRET=your-oauth-client-secret
OAUTH_TENANT_ID=your-tenant-id

# Monitoring
DATADOG_API_KEY=your-datadog-api-key
PAGERDUTY_API_KEY=your-pagerduty-api-key

# Feature Flags
ENABLE_SLACK_INTEGRATION=true
ENABLE_EMAIL_PROCESSING=true
ENABLE_API_AUTH=true

# Environment
ENVIRONMENT=development