# AutoSpecAI Frontend Environment Configuration
# Copy this file to .env.local (or .env.development/.env.production) and fill in your actual values

# API Configuration
REACT_APP_API_BASE_URL=https://your-api-gateway-url.execute-api.us-east-1.amazonaws.com/prod
REACT_APP_API_VERSION=v1
REACT_APP_DEFAULT_API_KEY=your-api-key-here

# Environment
REACT_APP_ENVIRONMENT=development

# WebSocket Configuration (if applicable)
REACT_APP_WEBSOCKET_URL=wss://your-websocket-url/ws

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG=true

# Build Configuration
GENERATE_SOURCEMAP=true
BUILD_PATH=build