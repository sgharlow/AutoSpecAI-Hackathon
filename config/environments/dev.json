{
  "environment": "dev",
  "aws": {
    "region": "us-east-1",
    "account_pattern": "*"
  },
  "application": {
    "stack_name": "AutoSpecAIStack-dev",
    "log_retention_days": 7,
    "lambda_timeout_minutes": {
      "ingest": 5,
      "process": 15,
      "format": 5,
      "slack": 3,
      "api": 1,
      "monitoring": 5
    },
    "lambda_memory_mb": {
      "ingest": 1024,
      "process": 3008,
      "format": 2048,
      "slack": 256,
      "api": 512,
      "monitoring": 512
    },
    "provisioned_concurrency": {
      "enabled": true,
      "configurations": {
        "process": {
          "min_capacity": 1,
          "max_capacity": 3,
          "target_utilization": 70.0,
          "warmup_schedule": "rate(2 hours)"
        },
        "format": {
          "min_capacity": 1,
          "max_capacity": 2,
          "target_utilization": 60.0,
          "warmup_schedule": "rate(4 hours)"
        },
        "api": {
          "min_capacity": 2,
          "max_capacity": 5,
          "target_utilization": 80.0,
          "warmup_schedule": "rate(30 minutes)"
        }
      }
    }
  },
  "monitoring": {
    "enabled": true,
    "x_ray_tracing": true,
    "detailed_monitoring": false,
    "alarm_thresholds": {
      "error_rate_percent": 15,
      "processing_time_seconds": 900,
      "low_throughput_requests": 0
    },
    "dashboard_refresh_seconds": 300
  },
  "features": {
    "slack_integration": false,
    "advanced_pdf_generation": true,
    "email_notifications": true,
    "api_authentication": false,
    "rate_limiting": false
  },
  "processing": {
    "file_limits": {
      "max_size_mb": 10,
      "allowed_extensions": [".pdf", ".docx", ".doc", ".txt"],
      "supported_mime_types": {
        "application/pdf": ".pdf",
        "application/vnd.openxmlformats-officedocument.wordprocessingml.document": ".docx",
        "application/msword": ".doc",
        "text/plain": ".txt"
      }
    },
    "rate_limiting": {
      "requests_per_hour": 100,
      "window_seconds": 3600,
      "burst_limit": 10,
      "enabled": false
    },
    "text_extraction": {
      "preview_char_limit": 500,
      "prompt_char_limit": 8000
    }
  },
  "bedrock": {
    "model_id": "anthropic.claude-3-5-sonnet-20241022-v2:0",
    "max_tokens": 4000,
    "temperature": 0.1,
    "top_p": 0.9,
    "timeout_seconds": 300
  },
  "branding": {
    "company_name": "AutoSpec.AI",
    "website_url": "https://autospec.ai",
    "support_email": "support@autospec.ai",
    "api_base_url": "https://dev-api.autospec.ai/v1",
    "logo_url": "",
    "color_scheme": {
      "primary": "#3498db",
      "secondary": "#2c3e50",
      "accent": "#e74c3c"
    }
  },
  "templates": {
    "email": {
      "subject_template": "[DEV] AutoSpec.AI - System Requirements for {filename}",
      "from_name": "AutoSpec.AI Development",
      "default_sender": "dev-api-user@autospec.ai"
    },
    "pdf": {
      "footer_template": "Generated by {company_name} - Development Environment",
      "title_template": "System Requirements Analysis - {filename}"
    },
    "error_messages": {
      "file_too_large": "File size exceeds the maximum allowed limit of {max_size_mb}MB",
      "unsupported_format": "Unsupported file format. Please use: {allowed_formats}",
      "processing_failed": "Document processing failed. Please try again or contact support."
    }
  },
  "s3": {
    "versioning": true,
    "encryption": "S3_MANAGED",
    "lifecycle_policies": {
      "delete_after_days": 30
    }
  },
  "dynamodb": {
    "billing_mode": "PAY_PER_REQUEST",
    "point_in_time_recovery": false,
    "backup_retention_days": 7
  },
  "api_gateway": {
    "rate_limit": 1000,
    "burst_limit": 2000,
    "quota_per_day": 10000,
    "cors_enabled": true
  },
  "integrations": {
    "slack": {
      "webhook_url": "",
      "signing_secret": "",
      "help_url": "https://dev.autospec.ai/help",
      "timeout_seconds": 300
    },
    "external_apis": {
      "plotly_cdn": "https://cdn.plot.ly/plotly-latest.min.js",
      "chart_timeout_seconds": 30
    }
  },
  "notifications": {
    "slack_webhook_url": "",
    "notification_email": "dev-notifications@example.com",
    "sns_topic_name": "AutoSpecAI-Alerts-dev",
    "ses_verified_domain": "gmail.com",
    "ses_from_email": "dev-sender@example.com"
  },
  "resource_naming": {
    "bucket_pattern": "autospec-ai-documents-{environment}-{account}",
    "table_prefix": "autospec-ai-{environment}",
    "function_prefix": "AutoSpecAI-{environment}"
  },
  "testing": {
    "integration_tests": true,
    "load_testing": false,
    "smoke_tests": true,
    "mock_external_services": true,
    "detailed_logging": true
  },
  "security": {
    "api_key_required": false,
    "cors_origins": ["*"],
    "max_request_size_mb": 10,
    "encryption_at_rest": true,
    "encryption_in_transit": true
  },
  "cost_optimization": {
    "enabled": true,
    "budget_alerts": {
      "monthly_budget": 50.0,
      "alert_thresholds": [0.5, 0.8, 0.95],
      "notification_email": "dev-team@autospec.ai",
      "slack_webhook": ""
    },
    "optimization_targets": {
      "cost_per_request": 0.005,
      "cost_per_document": 0.05,
      "efficiency_score_target": 80.0
    },
    "auto_optimization": {
      "enabled": false,
      "lambda_memory_optimization": true,
      "cache_optimization": true,
      "lifecycle_optimization": true
    },
    "monitoring": {
      "daily_cost_tracking": true,
      "anomaly_detection": false,
      "cost_allocation_tags": ["Environment", "Project", "Function"],
      "report_frequency": "weekly"
    }
  }
}